{
    "name": "SJTU Entry Exit Application Autofill",
    "version": "1.0",
    "description": "Autofill the Entry & Exit Application",
    "manifest_version": 2,
    "author": "Tony Chen",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
        "cookies", 
        "tabs", 
        "http://*/", 
        "https://*/", 
        "<all_urls>",
        "clipboardWrite",
        "clipboardRead",
        "notifications",
        "declarativeContent",
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {}
}